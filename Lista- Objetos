class Pessoa {
    private String nome;
    private int idade;
    private String cpf;
    private String email;
    
    public void andar() {}
    public void comer() {}
    public void dormir() {}
    public void falar() {}
}

class Animal {
    private String nome;
    private String especie;
    private int idade;
    private double peso;
    
    public void comer() {}
    public void dormir() {}
    public void emitirSom() {}
    public void mover() {}
}

class Carro {
    private String marca;
    private String modelo;
    private int ano;
    private String cor;
    private int velocidade;
    
    public void ligar() {}
    public void desligar() {}
    public void acelerar() {}
    public void frear() {}
}

class Livro {
    private String titulo;
    private String autor;
    private int numeroPaginas;
    private int paginaAtual;
    
    public void abrir() {}
    public void fechar() {}
    public void virarPagina() {}
    public void marcarPagina() {}
}

class Televisao {
    private String marca;
    private int tamanho;
    private int canal;
    private int volume;
    private boolean estado;
    
    public void ligar() {}
    public void desligar() {}
    public void mudarCanal() {}
    public void aumentarVolume() {}
    public void diminuirVolume() {}
}

class Celular {
